{
    "name":              "reich/upload",
    "description":       "A great class to upload your file or files to the server, could be integreted with laravel",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/edenreich",
    "keywords":          ["upload", "files", "file"],
    "authors": [
        {
            "name":     "Eden Reich",
            "email":    "eden.reich@gmail.com",
            "homepage": "https://github.com/edenreich/"
        }
    ],
    "require" : {
        "php": ">=5.6.0"
    },
	"require-dev": {
        "phpunit/phpunit": "~5.7"
    },
	"autoload": {
		"psr-4": {
			"Reich\\": "src/" 
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Upload\\Source\\Laravel\\Providers\\UploadServiceProvider"
            ],
            "aliases": {
                "Upload": "Reich\\Upload\\Source\\Laravel\\Facades\\UploadFacade"
            }
        }
    },
	"config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
